<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:orientation="vertical"
    android:background="#E9EAEF"
    >



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="130dp"
        android:background="#FFEB3B"
        android:orientation="vertical"


        >
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
android:layout_marginTop="40dp"

            >
            <androidx.cardview.widget.CardView
                android:layout_width="35dp"
                android:layout_height="35dp"
                android:layout_marginLeft="12dp"
                app:cardCornerRadius="20dp">
                <ImageView
                    android:layout_width="35dp"
                    android:layout_height="35dp"

                    android:src="@drawable/download">

                </ImageView>
            </androidx.cardview.widget.CardView>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="330dp"
                android:drawableRight="@drawable/search"
                android:drawableTint="@color/black">

            </TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
       android:drawableRight="@drawable/computer"
                android:drawableTint="@color/black"
                android:layout_marginLeft="20dp"
                >


            </TextView>

        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="20dp"
 >
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="PERSONAL"
                android:textStyle="bold"
                android:textSize="20dp"
                android:textColor="@color/black"
                android:layout_marginLeft="20dp">


            </TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="ORAGANIZATION"
                android:textStyle="bold"
                android:textSize="20dp"

                android:layout_marginLeft="60dp">

            </TextView>
    </LinearLayout>

    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="440dp"
      android:drawableRight="@drawable/ic_baseline_add_circle_24"
            android:drawableTint="@color/black"
            android:layout_marginTop="12dp"
            android:layout_marginBottom="12dp">

        </TextView>


    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        >
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="My Favourites"
            android:drawableRight="@drawable/menu"
            android:textSize="18dp"
            android:textStyle="bold"
            android:textColor="@color/black"
            android:layout_margin="12dp">

        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@color/white"
        android:layout_margin="10dp"
        >


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginLeft="40dp"
        android:layout_weight="1"
        android:layout_marginTop="8dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableStart="@drawable/email"
            android:layout_marginLeft="2dp"
            android:layout_gravity="center"
            app:drawableTint="@android:color/holo_blue_light">

        </TextView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
          android:text="Mail"
            android:layout_marginTop="4dp"
            android:layout_gravity="center"
            android:textStyle="bold"
            android:textColor="@color/black">

        </TextView>

      <ImageView
          android:layout_width="30dp"
          android:layout_height="30dp"
          android:src="@drawable/connect"
          android:layout_gravity="center"
          android:layout_marginTop="12dp"
          >

      </ImageView>
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="connect"
            android:layout_marginBottom="12dp"
            android:layout_gravity="center"
            android:textStyle="bold"
            android:textColor="@color/black">

        </TextView>

    </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"


            android:layout_marginTop="8dp">
      <ImageView
          android:layout_width="30dp"
          android:layout_height="30dp"
          android:layout_gravity="center"
          android:src="@drawable/r">

      </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Ring central"
                android:layout_gravity="center"
                android:textStyle="bold"

                android:textColor="@color/black">

            </TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableStart="@drawable/message"
                android:layout_marginLeft="2dp"
                android:layout_marginTop="12dp"
                android:layout_gravity="center"
                app:drawableTint="@android:color/holo_blue_light">

            </TextView>

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="12dp"
                android:text="cliq"
                android:textColor="@color/black"
                android:textStyle="bold">

            </TextView>

        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginRight="35dp"
            android:layout_weight="1"

         android:gravity="right"
            android:layout_marginTop="8dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableStart="@drawable/sale"
                android:layout_marginLeft="2dp"
                android:layout_gravity="center"
                app:drawableTint="@android:color/holo_blue_light">

            </TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:text="Point of sale"
                android:layout_gravity="center"
                android:textStyle="bold"
                android:textColor="@color/black">

            </TextView>




</LinearLayout>
    </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="12dp"
        android:orientation="horizontal">

    </LinearLayout>
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Sales and Support"
        android:drawableRight="@drawable/menu"
        android:textSize="18dp"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:layout_margin="12dp">

    </TextView>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@color/white"
        android:layout_margin="10dp"
        >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"

            android:gravity="center"
            android:layout_marginLeft="40dp">
           <ImageView
               android:layout_width="30dp"
               android:layout_height="30dp"
               android:src="@drawable/desk"
               android:layout_marginTop="12dp"
               >


           </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Desk"
                android:layout_marginBottom="12dp"
                android:layout_marginTop="12dp"
                android:textColor="@color/black"
                android:textStyle="bold">

            </TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            android:gravity="center">
        <ImageView
            android:layout_width="30dp"
            android:layout_marginTop="12dp"
            android:layout_height="30dp"
            android:src="@drawable/assest">

        </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Assest"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="12dp"
                android:textColor="@color/black"
                android:textStyle="bold">

            </TextView>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_marginRight="40dp"
            android:gravity="center">
      <ImageView
          android:layout_width="30dp"
          android:layout_height="30dp"
          android:src="@drawable/iq"
          android:layout_marginTop="12dp">

      </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sales Iq"
                android:layout_marginTop="12dp"
                android:layout_marginBottom="12dp"
                android:textColor="@color/black"
                android:textStyle="bold">

            </TextView>
        </LinearLayout>

        </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_margin="12dp"
 >
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Collaboration"
            android:drawableRight="@drawable/menu"
            android:textColor="@color/black"
            android:textStyle="bold"
            android:textSize="18dp">

        </TextView>
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:background="@color/white"
        android:layout_margin="10dp"
        >


        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginLeft="40dp"
            android:layout_weight="1"
            android:layout_marginTop="8dp">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableStart="@drawable/ic_baseline_drive_file_move_24"
                android:layout_marginLeft="2dp"
                android:layout_gravity="center"
                app:drawableTint="@android:color/holo_blue_light">

            </TextView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="World drive"
                android:layout_marginTop="4dp"
                android:layout_gravity="center"
                android:textStyle="bold"
                android:textColor="@color/black">

            </TextView>

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/booking"
                android:layout_gravity="center"
                android:layout_marginTop="12dp">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="booking"
                android:layout_marginBottom="12dp"
                android:layout_gravity="center"
                android:textStyle="bold"
                android:textColor="@color/black">

            </TextView>

        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_weight="1"


            android:layout_marginTop="8dp">
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_gravity="center"
                android:src="@drawable/sprint">

            </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Sprints"
                android:layout_gravity="center"
                android:textStyle="bold"

                android:textColor="@color/black">

            </TextView>

            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/notebook"
                android:layout_gravity="center"
                android:layout_marginTop="12dp">

            </ImageView>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginTop="5dp"
                android:text="notebook"
                android:textColor="@color/black"
                android:textStyle="bold">

            </TextView>

        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:layout_marginRight="35dp"
            android:layout_weight="1"

            android:gravity="right"
            android:layout_marginTop="8dp">
   <ImageView
       android:layout_width="30dp"
       android:layout_height="30dp"
       android:src="@drawable/group"
       android:layout_gravity="center">

   </ImageView>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:text="Meeting"
                android:layout_gravity="center"
                android:textStyle="bold"
                android:textColor="@color/black">

            </TextView>
            <ImageView
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/showtime"
                android:layout_gravity="center"
                android:layout_marginTop="12dp">

            </ImageView>


            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="12dp"
                android:text="showtime"
                android:textColor="@color/black"
                android:textStyle="bold">

            </TextView>






        </LinearLayout>
    </LinearLayout>

</LinearLayout>