<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity5"
    android:orientation="vertical">
    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:layout_margin="12dp">
<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="horizontal">



<androidx.appcompat.widget.AppCompatButton
    android:layout_width="100dp"
    android:layout_height="40dp"
    android:text="button"
    android:layout_gravity="center"
    android:background="#CE2753F8"
    android:textColor="@color/white">

</androidx.appcompat.widget.AppCompatButton>
    <androidx.appcompat.widget.AppCompatButton
        android:layout_width="200dp"
        android:layout_marginLeft="39dp"
        android:layout_height="40dp"
        android:text="button"
        android:background="#CE2753F8"
        android:textColor="@color/white"
        android:layout_gravity="center">

    </androidx.appcompat.widget.AppCompatButton>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:drawableStart="@drawable/heart"
        android:layout_gravity="center"
        android:layout_marginLeft="12dp"
        android:layout_marginRight="12dp">

    </TextView>
    <androidx.cardview.widget.CardView
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:cardCornerRadius="59dp"
        app:cardBackgroundColor="#E91E63"
        android:layout_marginLeft="8dp"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableStart="@drawable/person"
            android:layout_gravity="center">

        </TextView>
    </androidx.cardview.widget.CardView>
    <androidx.cardview.widget.CardView
        android:layout_width="80dp"
        android:layout_height="80dp"
        app:cardCornerRadius="59dp"
        app:cardBackgroundColor="#CE2753F8"
        android:layout_marginLeft="12dp"
        >
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableStart="@drawable/arrow_upward"
            android:layout_gravity="center">

        </TextView>
    </androidx.cardview.widget.CardView>
</LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:text="button"
                    android:layout_gravity="center"
                    android:background="#CE808185"
                    android:textColor="@color/white">

                </androidx.appcompat.widget.AppCompatButton>
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:text="true"
                    android:layout_gravity="center"
                    android:background="#CE2753F8"
                    android:textColor="@color/white"
                    android:layout_marginLeft="12dp">

                </androidx.appcompat.widget.AppCompatButton>
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:text="false"
                    android:layout_gravity="center"
                    android:background="#CE808185"
                    android:textColor="@color/white"
                    android:layout_marginLeft="12dp">

                </androidx.appcompat.widget.AppCompatButton>
                <RadioButton
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:layout_marginLeft="12dp"
                    android:layout_marginRight="12dp"
                    android:layout_gravity="center">

                </RadioButton>
                <androidx.cardview.widget.CardView
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    app:cardCornerRadius="59dp"
                    app:cardBackgroundColor="#4CAF50"
                    android:layout_marginTop="12dp"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:drawableStart="@drawable/call"
                        android:layout_gravity="center">

                    </TextView>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    app:cardCornerRadius="59dp"
                    app:cardBackgroundColor="#F44336"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="12dp"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:drawableStart="@drawable/add"
                        android:layout_gravity="center">

                    </TextView>
                </androidx.cardview.widget.CardView>

            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:drawableStart="@drawable/on"
                android:layout_gravity="center"
                >

            </TextView>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:drawableStart="@drawable/off"
                    android:layout_gravity="center"
                    >

                </TextView>


                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="100dp"
                    android:layout_height="40dp"
                    android:text="+ create"
                    android:layout_gravity="center"
                    android:background="@drawable/button2"
                    android:backgroundTint="#CE2753F8"
                    android:textColor="@color/white"
                    android:layout_marginLeft="12dp">

                </androidx.appcompat.widget.AppCompatButton>
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="110dp"
                    android:layout_height="30dp"
                    android:text="action chip"
                    android:layout_gravity="center"
                    android:background="@drawable/button2"
                    android:backgroundTint="#CE808185"
                    android:textColor="@color/white"
                    android:layout_marginLeft="12dp">

                </androidx.appcompat.widget.AppCompatButton>
                <CheckBox
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:clickable="true"
                    android:layout_gravity="center">

                </CheckBox>
                <androidx.cardview.widget.CardView
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    app:cardCornerRadius="59dp"
                    app:cardBackgroundColor="#F44336"
                    android:layout_marginLeft="34dp"
                    android:layout_marginTop="12dp"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:drawableStart="@drawable/end"
                        android:layout_gravity="center">

                    </TextView>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    app:cardCornerRadius="59dp"
                    app:cardBackgroundColor="@color/black"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="12dp"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:drawableStart="@drawable/add"
                        android:layout_gravity="center">

                    </TextView>
                </androidx.cardview.widget.CardView>
            </LinearLayout>
            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal">


                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="110dp"
                    android:layout_height="30dp"
                    android:text="outline chip"
                    android:layout_gravity="center"
                    android:background="@drawable/button2"
                  android:backgroundTint="#2A000000"
                    android:textColor="@color/black"
                   >

                </androidx.appcompat.widget.AppCompatButton>
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="140dp"
                    android:layout_height="30dp"
                    android:text="action chip"
                    android:layout_gravity="center"
                    android:background="@drawable/button2"
                    android:backgroundTint="#81808185"
                    android:textColor="@color/white"
                    android:layout_marginLeft="10dp"
                    android:drawableEnd="@drawable/cancel"
                    android:drawableTint="#8C8984"
                    android:drawableStart="@drawable/ic_baseline_location_on_24">

                </androidx.appcompat.widget.AppCompatButton>
                <androidx.appcompat.widget.AppCompatButton
                    android:layout_width="125dp"
                    android:layout_height="30dp"
                    android:text="action chip"
                    android:layout_gravity="center"
                    android:background="@drawable/button2"
                    android:backgroundTint="#CE808185"
                    android:textColor="@color/white"
                    android:drawableStart="@drawable/ic_baseline_location_on_24"
                    android:layout_marginLeft="10dp"
                    >

                </androidx.appcompat.widget.AppCompatButton>
                <androidx.cardview.widget.CardView
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    app:cardCornerRadius="59dp"
                    app:cardBackgroundColor="#CE2753F8"
                    android:layout_marginLeft="3dp"
                    android:layout_marginTop="12dp"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:drawableStart="@drawable/turn"
                        android:layout_gravity="center">

                    </TextView>
                </androidx.cardview.widget.CardView>
                <androidx.cardview.widget.CardView
                    android:layout_width="80dp"
                    android:layout_height="80dp"
                    app:cardCornerRadius="59dp"
                    app:cardBackgroundColor="#E9F44336"
                    android:layout_marginLeft="12dp"
                    android:layout_marginTop="12dp"
                    >
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:drawableStart="@drawable/create"
                        android:layout_gravity="center">

                    </TextView>

                </androidx.cardview.widget.CardView>

            </LinearLayout>
            <EditText
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:hint="Enter your name"
                android:textColor="@color/black"
                android:textColorHint="@color/purple_500"
                android:maxLines="1"
                android:maxLength="15"
                android:inputType="phone">
            </EditText>
        </LinearLayout>








    </ScrollView>

</LinearLayout>